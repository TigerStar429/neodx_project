{
  "name": "@neodx/codegen",
  "description": "Code generation API",
  "packageManager": "yarn@3.2.0",
  "version": "0.0.2",
  "type": "module",
  "main": "dist/index.cjs",
  "types": "dist/index.d.ts",
  "module": "dist/index.mjs",
  "source": "src/index.ts",
  "author": "Dmitry Remezov",
  "license": "MIT",
  "homepage": "https://github.com/secundant/neodx/tree/main/libs/codegen",
  "repository": {
    "type": "git",
    "url": "https://github.com/secundant/neodx.git",
    "directory": "libs/codegen"
  },
  "bugs": {
    "url": "https://github.com/secundant/neodx/issues"
  },
  "keywords": [
    "template",
    "codegen",
    "generator",
    "boilerplate",
    "neodx"
  ],
  "files": [
    "dist",
    "README.md",
    "package.json"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false,
  "scripts": {
    "add-example": "node scripts/add-example.mjs",
    "typecheck": "tsc --noEmit",
    "clean": "yarn dlx rimraf dist",
    "build": "libmake",
    "test": "NODE_OPTIONS='--experimental-vm-modules' jest",
    "lint": "eslint",
    "dev": "libmake -w"
  },
  "devDependencies": {
    "@neodx/config": "workspace:*",
    "@neodx/libmake": "workspace:*",
    "@types/ejs": "^3",
    "@types/jest": "29.1.2",
    "@types/node": "18.8.5",
    "@types/prettier": "^2.7.1",
    "@types/semver": "^7",
    "@types/tmp": "^0.2.3",
    "eslint": "8.25.0",
    "eslint-kit": "6.8.0",
    "jest": "29.1.2",
    "tmp": "0.2.1",
    "typescript": "4.8.4"
  },
  "dependencies": {
    "chalk": "5.1.2",
    "ejs": "3.1.8",
    "jsonc-parser": "3.2.0",
    "prettier": "2.7.1",
    "semver": "7.3.8"
  },
  "nx": {
    "targets": {
      "add-example": {
        "dependsOn": [
          "build"
        ]
      }
    }
  }
}
