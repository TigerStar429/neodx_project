{
  "name": "@neodx/svg",
  "packageManager": "yarn@3.2.0",
  "version": "0.0.1",
  "description": "Build optimized SVG sprites without pain",
  "source": "src/index.ts",
  "type": "module",
  "main": "dist/cjs/index.cjs",
  "module": "dist/esm/index.mjs",
  "types": "dist/types/index.d.ts",
  "exports": {
    "types": "./dist/types/index.d.ts",
    "import": "./dist/esm/index.mjs",
    "require": "./dist/cjs/index.cjs"
  },
  "bin": {
    "sprite": "bin.mjs"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "sideEffects": false,
  "scripts": {
    "typecheck": "tsc --noEmit",
    "clean": "yarn dlx rimraf dist",
    "build": "libmake",
    "test": "NODE_OPTIONS='--experimental-vm-modules' jest",
    "lint": "eslint src",
    "dev": "libmake watch"
  },
  "dependencies": {
    "@neodx/codegen": "workspace:*",
    "lilconfig": "2.0.6",
    "ora": "6.1.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "sade": "1.8.1",
    "svgo": "2.8.0",
    "svgson": "5.2.1",
    "tiny-glob": "0.2.9"
  },
  "devDependencies": {
    "@neodx/config": "workspace:*",
    "@neodx/libmake": "workspace:*",
    "@swc/helpers": "0.4.12",
    "@types/eslint": "^8.4.6",
    "@types/jest": "29.1.2",
    "@types/node": "18.8.5",
    "@types/prettier": "^2.7.1",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "@types/svgo": "^2.6.4",
    "eslint": "8.25.0",
    "eslint-kit": "6.8.0",
    "jest": "29.1.2",
    "prettier": "2.7.1",
    "typescript": "4.8.4"
  },
  "peerDependencies": {
    "prettier": "^2"
  },
  "peerDependenciesMeta": {
    "prettier": {
      "optional": true
    }
  }
}
