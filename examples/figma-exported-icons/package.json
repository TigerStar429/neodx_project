{
  "name": "example-figma-exported-icons",
  "version": "0.0.1",
  "private": true,
  "packageManager": "yarn@3.2.0",
  "type": "module",
  "sideEffects": false,
  "publishConfig": {
    "access": "restricted"
  },
  "dependencies": {
    "@neodx/figma": "workspace:*"
  },
  "scripts": {
    "export-icons": "figma export --verbose"
  },
  "nx": {
    "targets": {
      "export-icons": {
        "outputs": [
          "assets/icons"
        ],
        "inputs": [
          "{projectRoot}/assets/**/*.svg",
          "shared",
          "config"
        ],
        "dependsOn": [
          "^build"
        ]
      }
    }
  }
}
